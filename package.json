{
  "name": "oc-mall-theme",
  "version": "1.1.0",
  "description": "Official starter theme for the Mall e-commerce plugin ported to Tailwind 1.7.x",
  "main": "app.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:Voilaah/oc-mall-theme.git"
  },
  "homepage": "https://github.com/voilaah/oc-mall-theme",
  "bugs": {
    "url": "https://github.com/voilaah/oc-mall-theme/issues"
  },
  "dependencies": {
    "jquery": "^1.12.3",
    "postcss-for": "^2.1.1",
    "postcss-simple-vars": "^5.0.2",
    "tailwindcss": "^1.7.5"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^7.0.2",
    "glider-js": "^1.7.2",
    "laravel-mix": "^5.0.4",
    "moveto": "^1.8.2",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.3",
    "postcss-scss": "^2.1.1",
    "resolve-url-loader": "^3.1.0",
    "sal.js": "^0.7.4",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "smooth-scroll": "^16.1.3",
    "validate.js": "^0.13.1",
    "vue-template-compiler": "^2.6.11"
  },
  "author": "Tobias Kuendig <tobias@offline.swiss>",
  "license": "MIT"
}
